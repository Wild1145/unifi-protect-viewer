{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Unifi Protect Viewer",
  "minimum_chrome_version": "24.0.1307.0",
  "version": "2.0.2",
  "icons": {
    "16": "img/16.png",
    "128": "img/128.png"
  },
  "app": {
    "background": {
      "scripts": ["config.js", "main.js"]
    }
  },
  "permissions": [
    "webview",
    "fullscreen",
    "overrideEscFullscreen"
  ],
  "kiosk_enabled": true
}
